<h2>Available drones</h2>

{{#each drones}}
    <section class="drone-summary">
        <h3>{{name}}</h3>
        <p>
            Propellers: {{propellers}} <br>
            Max Speed: {{maxSpeed}} <br>
            
            <a href="/drones/{{_id}}/edit">Edit</a> 
        </p>
    </section>
{{/each}}